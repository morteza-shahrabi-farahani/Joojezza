<Window x:Class="Joojizza.Rating"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Joojizza"
        mc:Ignorable="d"
        Title="Rating" Height="385.214" Width="334.241">
    <Grid Margin="0,0,3.6,0">
        <ComboBox x:Name="foodCombo" HorizontalAlignment="Left" Margin="45,50,0,0" VerticalAlignment="Top" Width="226"/>
        <ComboBox x:Name="ratingCombo" HorizontalAlignment="Left" Margin="45,143,0,0" VerticalAlignment="Top" Width="226">
            <ComboBoxItem Content="1"></ComboBoxItem>
            <ComboBoxItem Content="2"></ComboBoxItem>
            <ComboBoxItem Content="3"></ComboBoxItem>
            <ComboBoxItem Content="4"></ComboBoxItem>
            <ComboBoxItem Content="5"></ComboBoxItem>
        </ComboBox>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Left" Margin="121,287,0,0" VerticalAlignment="Top" Width="75" Click="Save_Click"/>

    </Grid>
</Window>
